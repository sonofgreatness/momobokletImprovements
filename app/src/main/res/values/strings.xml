    <string name="quit_help">QUIT HELP</string>
    <string name="transactions">Transactions</string>
    <string name="record_transaction_help">You can record a transaction by filling out the form with the necessary transaction details ,
        hitting the submit button , then record customer\'s signature in the signature pad, confirm/edit transaction details in the following page and hit the record button to finish.</string>
    <string name="contact_us">Contact Us ( +268 ) 76911464</string>
